{
  "manifest_version": 3,
  "name": "Flix Bypass",
  "version": "1.0",
  "description": "Netflix bypass extension",
  "permissions": [
    "cookies",
    "activeTab",
    "storage",
    "webNavigation",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "logo.png"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}

